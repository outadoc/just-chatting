CREATE TABLE RecentEmote (
    `name` TEXT NOT NULL,
    `url` TEXT NOT NULL,
    `used_at` INTEGER NOT NULL,
    PRIMARY KEY (name)
);

getAll:
SELECT * FROM RecentEmote
ORDER BY used_at DESC;

insert:
INSERT OR REPLACE INTO RecentEmote (name, url, used_at)
VALUES (?, ?, ?);

cleanUp:
DELETE FROM RecentEmote
WHERE name NOT IN (
    SELECT name FROM RecentEmote
    ORDER BY used_at DESC
    LIMIT :limit
 );
